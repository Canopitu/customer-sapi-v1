#%RAML 1.0
title: customer-SAPI-raml
description: This API is used for connecting sfdc
version: 1.0.0
traits:
  common-header: !include /exchange_modules/0c025543-380e-49aa-9cf8-be80901e8e10/common-fragment/1.0.1/traits/common-headers-trait.raml
types:
  error-dataType: !include /exchange_modules/0c025543-380e-49aa-9cf8-be80901e8e10/common-fragment/1.0.1/datatypes/error-dataType.raml
  post-registration-request: !include /exchange_modules/0c025543-380e-49aa-9cf8-be80901e8e10/common-fragment/1.0.1/datatypes/post-registration-request-dataType.raml
  post-registration-response: !include /exchange_modules/0c025543-380e-49aa-9cf8-be80901e8e10/common-fragment/1.0.1/datatypes/post-registration-response-dataType.raml
/customers:
  /registration:
    post:
      is:
       - common-header
      body:
        application/json:
          type: post-registration-request
          example: !include /Request_examples/post-registration-request-example.json
      responses:
        201:
          body:
            application/json:
              type: post-registration-response
              example: !include /Request_examples/post-registration-response-example.json
        400:
          body:
            application/json:
              type: error-dataType
              example: !include /exchange_modules/0c025543-380e-49aa-9cf8-be80901e8e10/common-fragment/1.0.1/examples/error-400-response-example.json
               
        401:
          body:
            application/json:
              type: error-dataType
              example: !include /exchange_modules/0c025543-380e-49aa-9cf8-be80901e8e10/common-fragment/1.0.1/examples/error-401-response-example.json
        404:
          body:
            application/json:
              type: error-dataType
              example: !include /exchange_modules/0c025543-380e-49aa-9cf8-be80901e8e10/common-fragment/1.0.1/examples/error-404-response-example.json
        500:
          body:
            application/json:
              type: error-dataType
              example: !include /exchange_modules/0c025543-380e-49aa-9cf8-be80901e8e10/common-fragment/1.0.1/examples/error-500-response-example.json
